{
  "manifest_version": 3,
  "name": "Browse with Quote",
  "description": "Displays a new quote and its author each time you open a new tab.",
  "version": "1.0",
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "permissions": [
    "storage"
  ],
  "chrome_url_overrides": {
    "newtab": "newtab.html"
  }
}
